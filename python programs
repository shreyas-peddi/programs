import random
n=random.randint(0,10)
count=0
print('enter a number')
for i in range (1,6):
      x=int(input())
      if(n>x):
        print("you guessed small")
        count+=1
      elif(n<x):
        print("you guessed big")
        count=count+1
      else:
        print('you guessed the number in '+str(count)+' attempts.')
        break
if(count==6):        
    print('you did not guess the number')    
